<PictureContent>:
    orientation: 'vertical'
    padding: "10dp"
    spacing: "10dp"
    id:container
    adaptive_height:True

    TwoLineListItem:
        text: 'Add Picture'
        id: add_picture
        on_release: root.add_picture()
    TwoLineListItem:
        text: 'View Pictures'
        id: view_pictures
        on_release: root.view_pictures()