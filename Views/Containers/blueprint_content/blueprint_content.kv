<BlueprintContent>:
    orientation: 'vertical'
    padding: "10dp"
    spacing: "10dp"
    id:container
    adaptive_height:True

    TwoLineListItem:
        text: 'Add Blueprints'
        id: add_blueprints
        on_release: root.add_new_blueprints()

    TwoLineListItem:
        text: 'View Blueprints'
        id: view_blueprints
        on_release: root.open_blueprints()